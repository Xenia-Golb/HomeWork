// Задача 19
// Напишите программу, которая принимает пятизначное число на входе и проверяет, является ли оно палиндромом.
// 14212 -> нет
// 12821 -> да
// 23432 -> да

Console.Clear();
Console.Write("Введите пятизначное число ");
int number = int.Parse(Console.ReadLine()!) ;
int n1 = число / 10000;
int n2 = число % 10;
int n3 = (число / 1000) % 10;
int n4 = (число/10) % 10;
если (n1 == n2 && n3 == n4)
{
Console.WriteLine("Да");
}
иначе 
{
 Console.WriteLine ("Нет");
}
